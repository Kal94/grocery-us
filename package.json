{
  "name": "grocery-us",
  "version": "1.0.0",
  "engines": {
    "node": "12.16.2",
    "npm": "6.14.4"
  },
  "description": "Grocery shopping app, MERN",
  "main": "app.js",
  "scripts": {
    "client": "cd client && npm start",
    "server": "nodemon app.js",
    "build": "cd client && npm run build",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "start": "node app.js"
  },
